<div class="row home_content">
	<div class="span8">
        <h3 class="strikethrough-title"><span>{{ settings.sec_head1 }}</span></h3>
        <div class="slider horizontal" id="home_slider-premium">
            <div class="arrow left"><i class="fa fa-fw fa-caret-left"></i></div>
            <div class="arrow right"><i class="fa fa-fw fa-caret-right"></i></div>
            <div class="slider-wrapper">
                <div class="slider-track">
                    {% for product in collections.[settings.frontpage_collection].products %}
                        <div class="slider-product">
                            <a class="shop-now" href="{{ product.url }}">
                                <div class="slider-product-image">
                                    <img src="{{ product.featured_image | product_img_url: 'medium' }}" alt="{{ product.title | escape  }}" />
                                </div>
                                <div class="slider-product-title">{{ product.title | escape  }}</div>
                                <div class="slider-product-price">{{ product.price | money }}</div>
                            </a>
                        </div>  
                    {% endfor %}
                </div>
            </div>
        </div>
      
