<script>{% if product %}if(typeof pn==="undefined"){pn={};}if(typeof pn.cP==="undefined"){pn.cP={};}pn.cP[{{product.id}}]={"id":{{product.id}},"h":"{{product.handle}}","i":"{{product.images.first}}","t":[{%for t in product.tags%}"{{t | escape}}",{%endfor%}],"v":[{%for v in product.variants%}{"id":{{v.id}},"m":"{{v.inventory_management}}","p":"{{v.inventory_policy}}","q":{{v.inventory_quantity}},},{%endfor%}]}{% endif %}</script>