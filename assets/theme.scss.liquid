.dropdown-menu { background: $dropdownBg; color: $dropdownText; @include box-shadow (0 4px 10px rgba(0,0,0,0.15)); @include border-radius($radius); padding: 10px 20px; z-index: 999; border: 0 none;}
.dropdown-menu ul {list-style: none; margin: 0;}
ul.dropdown-menu>li {margin-left: -20px; margin-right: -20px; padding: 5px 20px;}
ul.dropdown-menu>li.active,
ul.dropdown-menu>li.selected {background: $dropdownBgActive; color: $dropdownTextActive; font-weight: 700;}


/* ##Header ================================================================= */
