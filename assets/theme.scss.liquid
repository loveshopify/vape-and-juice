/* Slick JS */
/* Slider */

.slick-slider {
    position: relative;
    display: block;
    box-sizing: border-box;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -ms-touch-action: pan-y;
    touch-action: pan-y;
    -webkit-tap-highlight-color: transparent;
}
.slick-list {
    position: relative;
    overflow: hidden;
    display: block;
    margin: 0;
    padding: 0;

    &:focus {
        outline: none;
    }

    &.dragging {
        cursor: pointer;
        cursor: hand;
    }
}
.slick-slider .slick-track,
.slick-slider .slick-list {
    -webkit-transform: translate3d(0, 0, 0);
    -moz-transform: translate3d(0, 0, 0);
    -ms-transform: translate3d(0, 0, 0);
    -o-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
}

.slick-track {
    position: relative;
    left: 0;
    top: 0;
    display: block;

    &:before,
    &:after {
        content: "";
        display: table;
    }

    &:after {
        clear: both;
    }

    .slick-loading & {
        visibility: hidden;
    }
}
.slick-slide {
    float: left;
    height: 100%;
    min-height: 1px;
    [dir="rtl"] & {
        float: right;
    }
    img {
        display: block;
        width: 100%;
    }
    &.slick-loading img {
        display: none;
    }

    display: none;

    &.dragging img {
        pointer-events: none;
    }

    .slick-initialized & {
        display: block;
    }

    .slick-loading & {
        visibility: hidden;
    }

    .slick-vertical & {
        display: block;
        height: auto;
        border: 1px solid transparent;
    }
}
.slick-arrow.slick-hidden {
    display: none;
}
/* Arrows */

.slick-prev,
.slick-next {
    @extend .slider-arrow;
    @include opacity(0);
    z-index: 9;
    &.slick-disabled {
      pointer-events: none;
      &:before {
        opacity: 0.2 !important;
      }
    }
}

.slick-slider:hover .slick-arrow {@include opacity(1);}

.slick-prev {
    left: -5px;
    @media (max-width: 991px) {
      left: 0;
    }
    &:before {
        content: '\f104';
    }
}

.slick-next {
    right: -5px;
    @media (max-width: 991px) {
      right: 0;
    }
    &:before {
        content: '\f105';
    }
}
.slick-slider.slick-vertical {padding-bottom: 38px;}
.slick-vertical .slick-arrow {position: absolute; @include opacity(1); @include border-radius(0); height: 30px; top: auto; bottom: 0; width: calc(50% - 5px);}
.slick-vertical .slick-arrow:before {line-height: 28px;}
.slick-vertical .slick-prev {left: 0;}
.slick-vertical .slick-next {right: 0;}
.slick-vertical .slick-prev:before {content: '\f106';}
.slick-vertical .slick-next:before {content: '\f107';}

/* Dots */

.slick-dots {
    list-style: none;
    display: block;
    text-align: center;
    padding: 10px 0 0;
    margin: 0;
    width: 100%;
    line-height: 0;
    li {
        position: relative;
        display: inline-block;
        margin: 0 4px;
        padding: 0;
        cursor: pointer;
        button {
            background: transparent;
            display: block;
            height: 14px;
            width: 14px;
            outline: none;
            line-height: 0;
            font-size: 0;
            color: transparent;
            border: 3px solid rgba(0,0,0,0.3);
            cursor: pointer;
            @include border-radius(50%);
            &:hover, &:focus {
                background-color: transparent;
                border-color: $colorPrimary;
            }
        }
        &.slick-active button {
            background-color: $colorPrimary;
            border-color: $colorPrimary;
            @include opacity(0.9);
        }
    }
}


