/* ##Navigation =========================================================== */
$colorNavWrapper: {{ settings.wrap_nav_bg }};
$colorNavBg: {{ settings.nav_bg }};
$colorNavText: {{ settings.nav_text }};
$colorNavTextHover: {{ settings.nav_text_hover }};
$colorNavTextFixed: {{ settings.nav_text_fixed }};
$colorNavDropdownBg: {{ settings.nav_dropdown_bg }};
$colorNavDropdownText: {{ settings.nav_dropdown_text }};
$colorNavDropdownTextHover: {{ settings.nav_dropdown_text_hover }};
$colorNavMobileBg: {{ settings.navi_mobile_bg }};
$colorNavMobileText: {{ settings.navi_mobile_color }};
$colorNavMobileTextHover: {{ settings.navi_mobile_color_hover }};
$colorNavMobileBorder: {{ settings.navi_mobile_border }};
$widthNavMobile: 320px;

.header-navigation {background-color: $colorNavBg; position: relative;} //$colorNavWrapper;
.site-nav-wrapper {background-color: $colorNavBg; color: $colorNavText;} 
.site-nav {font-size: $baseFontSize; cursor: default; margin: 0;}
.site-nav li{margin: 0; display: block;}
.site-nav>li{position: relative; display: inline-block; padding: 12px 15px;}
.site-nav>li:first-child {padding-left: 0;}
.site-nav>li:last-child {padding-right: 0;}
.site-nav>li>.nav-item-link {margin: 0; position: relative;}
.site-nav>li>.nav-item-link>a {display: block; text-decoration: none; padding: 0; line-height: 30px; white-space: nowrap; font-family: $navigationFontFamily; font-size: $navigationFontSize; font-weight: $navigationFontWeight; text-transform: $navigationTextTransform; color: $colorNavText; position: relative;}
/* .site-nav>li:hover>.nav-item-link, */
/* .site-nav>li:hover>.nav-item-link>a, */
.site-nav>li.current>.nav-item-link,
.site-nav>li.current>.nav-item-link>a {color: $colorNavTextHover; text-decoration: none;}
.site-nav li.dropdown>.nav-item-link>a:after {content: '\f107'; @extend .icon-font-awesome;}
.site-nav>li>.site-nav-dropdown {top: 100%; @include border-top-radius (0);}
.site-nav-mobile .site-nav  {display: inline-block;}
.site-nav-wrapper .tp-message-empty {margin: 15px 0;}

@media (min-width: 992px) {
  .site-nav>li>.nav-item-link:before {content: ""; display: block; height: 0; position: absolute; bottom: 0; left: 50%; right: 50%; border-bottom: 2px solid transparent; @include transition(all 300ms);} 
  .site-nav>li:hover>.nav-item-link:before {left: 1px; right: 1px; border-color: inherit;}
  .site-nav>li:hover>.nav-item-link:after {content: ""; height: 250%; position: absolute; top: 100%; left: 0; right: 0;}
}

@media (max-width: 991px) {
  .site-nav-wrapper { position: fixed; top: 0; bottom: 0; left: -100%; z-index: 9999; overflow-y: auto; overflow-x: hidden; padding-top: 60px; max-width: 98vw; width: $widthNavMobile; background-color: $colorNavMobileBg; @include transition(all 300ms);}
  .site-nav-wrapper.slide-active  {left: 0; z-index: 999999999999999;}
  .site-nav {padding: 0 25px;}
  .site-nav>li {display: block; padding: 0; border-bottom: 1px solid $colorNavMobileBorder;}
  .site-nav>li:last-child {border-bottom: 0 none;}
  .site-nav>li>.nav-item-link {padding: 15px 20px 15px 0; }
  .site-nav>li>.nav-item-link>a {display: inline-block; max-width: 100%; color: $colorNavMobileText;}
  .site-nav li .tp-mega-heading {color: $colorNavMobileText; padding-right: 20px;}
  .site-nav li .nav-item-link .icon-plus,
  .site-nav li .tp-mega-heading .icon-plus {position: absolute; top: 50%; right: -5px; padding: 0 5px; cursor: pointer; margin-top: -15px;}
  .site-nav li .nav-item-link .icon-plus:before,
  .site-nav li .tp-mega-heading .icon-plus:before {content: '\0046'; @include icon-font-peaces(15px); line-height: 30px;}
  .site-nav li .tp-mega-heading.open .icon-plus:before,
  .site-nav li .nav-item-link.open .icon-plus:before {content: '\0047';}
  .site-nav li.dropdown>.nav-item-link>a:after {display: none;}
  .tp-menu-mobile>div {border-color: $colorNavMobileBorder;}
  .tp-menu-mobile {background-color: $colorNavMobileBg;}
  .site-nav>li>.nav-item-link,
  .tp-menu-mobile .tp-col-currency .visible-phone,
  .tp-menu-mobile button {color: $colorNavMobileText;}
  .site-nav>li>.nav-item-link.current,
  .site-nav>li>.nav-item-link:hover,
  .tp-menu-mobile button:hover {color: $colorNavMobileTextHover;}
  .site-nav .site-nav-dropdown {position: static; background-color: lighten($colorNavMobileBg,10%); @include border-radius(0); @include box-shadow(none); @include border-radius(0);}
  .site-nav .site-nav-dropdown a {color: $colorNavMobileText;}
  .site-header-mobile {width: $widthNavMobile;}
  .site-nav-backdrop {position: fixed; top: 0; left: 0; bottom: 0;  right: 0; background-color: #000; z-index: 9998; @include opacity(0.5); @include transition;}
}


/* ================ Dropdowns ================ */
.site-nav-dropdown {display: none; margin: 0; }
.site-nav-dropdown ul {margin: 0;}
.site-nav-dropdown li > a { font-weight: 400; font-size: 14px; }
.site-nav-dropdown li li {padding-top: 5px; padding-bottom: 5px;}
.site-nav-dropdown li.dropdown>a:after {content: '\f105'; position: absolute; right: 10px; top: 50%; top: 50%; margin-top: -7px;}
.site-nav-dropdown a { color: $colorNavDropdownText; text-decoration: none; }
.site-nav-dropdown .grid-sidebar .grid-image {width: 100px}

/* .site-nav-dropdown a:hover,
.site-nav-dropdown a:active,
.site-nav-dropdown a:focus { color:$colorNavDropdownTextHover; } */

@media (min-width: 992px) {
  .site-nav-dropdown li { padding: 8px 20px; }
  .site-nav-dropdown {position: absolute; left: 0; top: 100%; z-index: 999; background-color: $colorNavDropdownBg; width: 240px; text-align: left; padding: 15px 0; @include border-radius ($radius); @include box-shadow(0 1px 4px rgba(0, 0, 0, 0.1))}
  .site-nav-dropdown>.dropdown>.site-nav-dropdown {top: 0; left: 100%;}
}


/* ##Mega Menu ===================================================== */
.tp-mega-heading {color: $colorNavDropdownText; font-size: $baseFontSize - 1px; font-weight: 700; line-height: 18px; margin: 0; padding: 15px 0; display: block; text-transform: uppercase; position: relative;}
/* .dropdown-menu li:hover,
.site-nav-dropdown li:hover{ color:$colorNavDropdownTextHover;} */

.dropdown.mega-menu .site-nav-dropdown .row {margin-left: -10px; margin-right: -10px;}
.dropdown.mega-menu .site-nav-dropdown .row>[class*="col-"] {padding-left: 10px; padding-right: 10px;} 
.dropdown.mega-menu .site-nav-dropdown .column img {height: auto; max-width: 100%;}
.dropdown.mega-menu .site-nav-dropdown .banner,
.dropdown.mega-menu .site-nav-dropdown .sample {display: block; clear: both; width: 100%;}
.dropdown.mega-menu .site-nav-dropdown .banner+.banner,
.dropdown.mega-menu .site-nav-dropdown .sample+.sample {margin-top: 20px;} 
.dropdown.mega-menu .site-nav-dropdown .column>a>span { display:block; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
/* .dropdown.mega-menu .site-nav-dropdown .column>a:hover,
.dropdown.mega-menu .site-nav-dropdown .column a:hover { color:$colorNavDropdownTextHover;} */
.dropdown.mega-menu .site-nav-dropdown .column ul li { padding: 5px 0; background: none; border: 0;}
.dropdown.mega-menu .site-nav-dropdown .column ul li a { color: $colorNavDropdownText; font-size: $baseFontSize - 1px; font-weight: 400; line-height: 20px; display: block; }
.dropdown.mega-menu.mega-menu-product-list .site-nav-dropdown .sca-qv-button {display: none !important; visibility: hidden !important;}
.dropdown.mega-menu.mega-image-cate .site-nav-dropdown .cate-image-link,
.dropdown.mega-menu.mega-image-cate .site-nav-dropdown .sample {display: block; margin-top: 15px;}
@media (min-width: 992px) {
  .tp-mega-heading {padding-top: 0;}
  .tp-mega-heading .plus-icon {display: none;}
  .dropdown:hover>.site-nav-dropdown { display:block;  -moz-animation: fadeIn .3s ease-in ;  -webkit-animation: fadeIn .3s ease-in ;  animation: fadeIn .3s ease-in;}
  .dropdown.mega-menu .site-nav-dropdown { width: 100vw; padding: 30px 0 20px; background-color:$colorNavDropdownBg; color:$colorNavDropdownText; border:1px solid darken($colorNavDropdownBg,10%); @include border-radius(0);}
  .dropdown.simple-dropdown .site-nav-dropdown .dropdown .tp-mega-heading {font-size: inherit; font-weight: inherit; padding: 0; text-transform: none; position: relative;}
  .dropdown.simple-dropdown .site-nav-dropdown .dropdown .tp-mega-heading>a:after {content: '\f105'; font: 14px/1 FontAwesome; display: inline-block; position: absolute; top: 50%; right: -7px; margin-top: -7px;}
  .dropdown.mega-menu .site-nav-dropdown .column {width: 20%; float: left; padding-right: 20px; padding-bottom: 25px;}
  .dropdown.mega-menu .site-nav-dropdown .site-nav-dropdown {position: static; display: block; border: 0 none; @include box-shadow(none); padding: 0;} 
  .dropdown.mega-menu.mega-menu-product-list .site-nav-dropdown .column,
  .dropdown.mega-menu.mega-menu-left-banner .site-nav-dropdown .column {width: 25%; padding-bottom: 25px;}
  .dropdown.mega-menu.mega-menu-product-list .site-nav-dropdown .column-link-list {padding-right: 25px;} 
  .dropdown.mega-menu.mega-menu-product-list .site-nav-dropdown .column-product-list {border-left: 1px solid $colorBorder; padding-left: 25px;}
  .dropdown.mega-menu.mega-menu-right-banner .site-nav-dropdown .column,
  .dropdown.mega-menu.mega-menu-text-banner .site-nav-dropdown .column,
  .dropdown.mega-menu.mega-menu-video .site-nav-dropdown .column {width: 33.333%;} 
  .dropdown.mega-menu.mega-menu-left-banner .site-nav-dropdown .column-banner {padding-right: 25px;}
  .dropdown.mega-menu.mega-image-cate-2 .site-nav-dropdown .column {width: 50%;}
}
@media (max-width: 991px) {
  .tp-mega-heading {@extend .dropdown-menu-heading;}
  .dropdown.simple-dropdown .site-nav-dropdown>li {padding-top: 5px; padding-bottom: 5px;}
  .dropdown.mega-menu .site-nav-dropdown .column {position: relative;}
  .dropdown.mega-menu .site-nav-dropdown,
  .dropdown .site-nav-dropdown { border: 0 none; margin-left: -25px; margin-right: -25px; width: calc(100% + 50px) !important; padding: 15px 25px;}
  .dropdown.mega-menu .site-nav-dropdown .container {padding: 0; width: 100%;}
  .dropdown.mega-menu .site-nav-dropdown .column ul li a {color: $colorNavMobileText;}
  .dropdown.mega-menu .site-nav-dropdown .tp-mega-cate-heading .icon-plus {top: 10px; margin-top: 0;}
  .dropdown.mega-menu.mega-image-cate .column+.column {padding-top: 20px;} 
}

.label-menu {font-size: 10px; font-weight:700; text-transform: uppercase; @include border-radius(2px);  color: #fff;  position: absolute;  top: -10px; right: -5px;  padding: 2px 3px; display: inline-block; line-height: 1; @include transition(all 300ms); }
.label-menu.label-new {background: $colorProductLabelNew;}
