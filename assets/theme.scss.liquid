.section-header h1,
.section-header .h1,
.section-header .section-header-title { font-family: $headingPageFontFamily; font-size: $headingPageFontSize; font-weight: $headingPageFontWeight; text-transform: $headingPageTextTransform; margin-bottom: 15px;
}


@include at-query ($max, 991px) {
