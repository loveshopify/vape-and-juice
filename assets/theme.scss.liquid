/* ##Gallery Page ================ */
.tp-gallery-list {list-style: none; margin: {{ settings.gallery_gutter_item | divided_by: -2 }}px;}
.tp-gallery-list>li {float: left; padding: {{ settings.gallery_gutter_item | divided_by: 2 }}px;
    width: {{ 100.0 | divided_by: settings.gallery_item_per_row | round: 2 }}%;}
.tp-gallery-list>li>figure {position: relative;}
.tp-gallery-list>li a {display: block; @include transition;}
.tp-gallery-list>li .fancybox:hover {@include opacity(0.9);}
.tp-gallery-list>li .tint {position: absolute; font-size: 0; line-height: 0;}
.tp-gallery-list>li .tint:before {content: "\0046"; @include icon-font-peaces(35px); color: #fff; text-shadow: 0 0 1px rgba(0,0,0,0.3); animation-name: zoomInOut; -webkit-animation-name: zoomInOut; animation-duration: 500ms; -webkit-animation-duration: 500ms; @include transition; animation-iteration-count: infinite; -webkit-animation-iteration-count: infinite; animation-direction: alternate; -webkit-animation-direction: alternate;}
