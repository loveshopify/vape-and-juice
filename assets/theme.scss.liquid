/* ##Popup Newsletter ================ */
$newsletterBg: {{ settings.popup_newsletter_bgcolor }};
$newsletterHeading: {{ settings.popup_newsletter_heading_color }};
$newsletterText: {{ settings.popup_newsletter_color }};
$newsletterTextHover: {{ settings.popup_newsletter_color_hover }};
$newsletterInputBg: {{ settings.input_common_bg_color }};
$newsletterInputText: {{ settings.input_common_color }};
$newsletterInputBorder: {{ settings.input_common_border_color }};

.newsletter-modal { display:none; height:100%; left:0; position:fixed; top:0; width:100%; z-index: 9999999; text-align: center; font-size: 0; overflow: auto;}
.newsletter-modal .modal-overlay {background:#000; height:100%; position:fixed; width:100%; zoom:1; top: 0; left: 0; @include opacity (0.5);}
.newsletter-modal .modal-window { display: inline-block; vertical-align: middle; position:relative; width: 600px; max-width:90%; margin: 30px 0;}
.newsletter-modal:before {content: ""; height: 100%; width: 0; display: inline-block; vertical-align: middle;}

.newsletter-modal .modal-content {padding:0; @include border-radius(0); background-color: $newsletterBg; @include box-shadow(none); text-align: center; border: 0 none; font-size: $baseFontSize;}
.newsletter-modal .modal-content .close {height: 30px; width: 30px; position: absolute; z-index: 1; right: 15px; top: 15px; cursor: pointer; opacity: 1; text-align: center; background: none; @include border-radius(0); text-shadow: none; padding: 0; color: $newsletterHeading;}
.newsletter-modal .modal-content .close:hover {color: $newsletterTextHover;}
.newsletter-modal .modal-content .close:before {content: '\0041'; @include icon-font-peaces(17px); line-height: 30px; display: block;}

.newsletter-modal .modal-content img:hover {@include opacity(1);}
.newsletter-modal .modal-content .newsletter-content { padding: 40px 25px;}
.newsletter-modal .modal-content .display-table {height: 100%; text-align: center;}
.newsletter-modal .modal-content .newsletter-heading  { font-size: $baseFontSize * 2; margin-bottom: 20px; font-weight: 700; color: $newsletterHeading;}
.newsletter-modal .modal-content .message {font-size: $baseFontSize + 2px; margin-bottom: 1.5em; color: $newsletterText;}
.newsletter-modal .modal-content .message strong {color: $newsletterTextHover;}
.newsletter-modal .modal-content .newsletter-msg {color: $newsletterText; display: inline-block; line-height: 17px;}
.newsletter-modal .modal-content .newsletter-msg:hover {color: $newsletterTextHover; text-decoration: none;}
.newsletter-modal .modal-content .newsletter-msg .icon {display: inline-block; width: 16px; height: 16px; padding: 3px; border: 1px solid lighten($newsletterText, 20%); margin-right: 10px; vertical-align: top; @include border-radius(2px);}
.newsletter-modal .modal-content .newsletter-msg .icon:before {content: ""; display: block; width: 8px; height: 8px; background-color: $newsletterTextHover;}
.newsletter-modal .modal-content .input-group {margin-bottom: 40px; text-align: center;}
#newsletter-modal .modal-content input[type="email"] {border: 1px solid $newsletterInputBorder; width: 100%; @include border-radius($radius 0 0 $radius); background-color: $newsletterInputBg; color: $newsletterInputText; height: 50px;}
#newsletter-modal .modal-content input[type="submit"] {width: 100%; text-align: center;  overflow: inherit; position: relative; z-index: 0; line-height: 50px; @include border-radius(0 $radius $radius 0);}

@media (min-width: 992px) {
  .newsletter-modal .modal-content .newsletter-content {padding: 50px;}
}

{% if settings.popup_newsletter_image_position == 'under_text' %}
.newsletter-modal .modal-content {background-image: url({{ "bg_popup_newsletter.png" | asset_url }}); background-repeat: no-repeat; background-position: center center; background-size: cover;}
{% endif %}

{% if settings.popup_newsletter_image_position == 'left' or settings.popup_newsletter_image_position == 'right' %}
.newsletter-modal .modal-window {width: 800px;}
.newsletter-modal .modal-content {display: table; width: 100%; }
.newsletter-modal .modal-content .newsletter-image,
.newsletter-modal .modal-content .newsletter-content {display: table-cell; vertical-align: middle;}
.newsletter-modal .modal-content .newsletter-image {width: 40%; position: relative;}
.newsletter-modal .modal-content .newsletter-image img {position: absolute; top: 0; left: 0; width: 100%; height: 100%; object-fit: cover; z-index: 0;}
@media (max-width: 599px) {
  .newsletter-modal .modal-content .newsletter-image {display: none;}
}
{% endif %}

{% if settings.popup_newsletter_image_position == 'top' %}
.newsletter-modal .modal-window {width: 500px;}
.newsletter-modal .modal-content .newsletter-content {padding-top: 30px; padding-bottom: 30px;}
.newsletter-modal .modal-content .input-group {margin-bottom: 2em;}
{% endif %}



