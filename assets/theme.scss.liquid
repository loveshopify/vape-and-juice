/* ##Pagination ================================================================ */
.pagination {clear: both; margin: 20px 0; text-align: center; display: block;}
.pagination>span {display: inline-block; vertical-align: middle; }
.pagination a,
.pagination .page.current,
.pagination .deco {padding: 9px 5px; display: inline-block; min-width: 40px; text-decoration: none; line-height: 20px; font-size: $baseFontSize; color: $colorLight; border: 1px solid $colorBorder; @include border-radius($radius);}
.pagination>.page.current{font-weight: 700; background-color: $colorPrimary; color: $colorTextonPrimary; border-color: $colorPrimary;}
.pagination>span:hover a,
.pagination>span:focus a {color: $colorPrimary;}
.pagination>span.prev a,
.pagination>span.next a {font-size: 0; line-height: 0;}
.pagination>span.prev a:before,
.pagination>span.next a:before {@include icon-font-peaces(12px); line-height: 20px;}
.pagination>span.prev a:before {content: '\0049';}
.pagination>span.next a:before {content: '\004a';}

{% if settings.infinite_scroll_enable %}
.pagination,
.tp-pagination-bottom {display: none;}
{% endif %}


/* ##Load More ============================================================= */
.ias_trigger {clear: both; padding: 30px 15px 10px;}
.ias_trigger a,
.ias-noneleft span {display: table; max-width: 350px; margin: 0 auto;}
.ias-noneleft {clear: both; padding-top: 30px; }
.ias-noneleft span {font-size: $baseFontSize +2; background-color: #f5f5f5; line-height: $btnHeight;
@include border-radius($btnRadius); padding-left: 20px; padding-right: 20px;}


/* ##GRID PRODUCT  ================================================================== */
/* ##GRID PRODUCT  ================================================================== */

$productItemPerRow: {{ settings.product_item_per_row }};
$productItemPerRow2: {{ settings.product_item_per_row_2 }};
$productItemGutter: {{ settings.product_item_gutter | remove: 'px' }}px;

/* Element ================================= */
.grid-image { display: block; margin: 0 auto;}
.grid-image img { display: block;  margin: 0 auto;  width: auto;  max-height: 100%; height:auto; position: static !important; }
.grid-image img:not(:first-child){display: none;}
.grid-image a > img:not(:first-child){ display: none; }

.grid-item-image {position: relative;}

/* Image Swatch */
.item-color-swatch {display:block;width:100%; clear: both; }
.item-color-swatch ul {margin: -3px;}
.item-color-swatch ul li { list-style-type:none;  padding: 3px; display: inline-block; line-height: 0;}
.item-color-swatch ul li span {display:block; width: 15px; height: 15px; cursor: pointer; @include border-radius(10px); font-size: 0; position: relative; background-size: cover;}
.item-color-swatch ul li span:hover {@include box-shadow(0 0 0 2px rgba(0,0,0,0.8));}

/* Product Label */
.item-label {position: absolute; right: 10px; top: 10px; margin: 0;}
.item-label li { list-style: none; font-weight: 700; line-height: 16px; text-transform: uppercase; padding: 2px 5px;  margin-bottom: 5px; text-align: center; font-size: 0; border-style: solid; border-width: 2px; }
.item-label li:before {content: ""; display: inline-block; height: 100%; vertical-align: middle; width: 0; line-height: 0;}
.item-label li>span {display: inline-block; vertical-align: middle; width: 100%; font-size: 12px;}

{% if settings.product_label_style == 'square' %}
.item-label li {color: #fff; @include border-radius($radius);}
.item-label .label-sale {background-color: $colorProductPriceSale; border-color: $colorProductPriceSale;}
.item-label .label-new { background-color: $colorProductLabelNew; border-color: $colorProductLabelNew; }
{% elsif settings.product_label_style == 'circle' %}
.item-label li {width: 42px; height: 42px; line-height: 1; color: #fff; @include border-radius(50%);}
.item-label .label-sale {background-color: $colorProductPriceSale; border-color: $colorProductPriceSale;}
.item-label .label-new { background-color: $colorProductLabelNew; border-color: $colorProductLabelNew; }
.item-label .sold-out>span {font-size: 10px;}
{% else%}
.item-label .label-sale {color: $colorProductPriceSale; border-color: $colorProductPriceSale; }
.item-label .label-new { color: $colorProductLabelNew; border-color: $colorProductLabelNew; }
{% endif %}


/* Wishlist + Compare  */
.tp-compare-wrap,
.tp-wishlist-wrap {display: inline-block;}

.tp-wishlist,
.tp-compare { font-size: 0; display: inline-block; text-align: center; width: 40px; height: 40px; line-height: 40px; padding: 0; background-color: #fff; border: 1px solid $colorBorder; position: relative; cursor: pointer; @include border-radius($radius);}

.tp-wishlist:before,
.tp-compare:before {@include icon-font-peaces(16px);  width: 100%;  height: 100%; line-height: 40px; }

.tp-wishlist:before{ content: "\004d";  }
.tp-compare:before { content: "\0046"; font-size: 15px;}

.tp-wishlist.checked-wishlist,
.tp-compare.checked-compare { pointer-events: none; }

.tp-wishlist.checked-wishlist:after,
.tp-compare.checked-compare:after { content: "\004f";  @include icon-font-peaces(10px);  position: absolute;  top: 5px; right: 5px;  width: 14px; height: 14px; line-height: 14px; text-align: center; color: $colorTextonPrimary; background: $colorPrimary; @include border-radius(10px); }

.tp-product-item-price { font-family: $productGridPriceFontFamily; font-size: $productGridPriceFontSize; font-weight: $productGridPriceFontWeight; color: $colorProductPrice; line-height: 1;}
.tp-product-item-price strong {display: none;}
.tp-product-item-price s { padding-top: 2px; color: $colorProductPriceOld; font-size: 14px; font-weight: 700; margin-left: 5px;}
.tp-product-item-price.tp-has-sale-price { color: $colorProductPriceSale; }
.tp-product-available {display: none;}

.spr-badge { clear: both; display: block;}
.spr-badge-starrating {margin-right: 0; font-size: $baseFontSize - 2px;}
.spr-badge-starrating>i {margin: 0 1px;}

.spr-icon {color: $colorProductRatting;}
.spr-badge-caption {display: inline-block; vertical-align: middle; font-size: 10px; padding-left: 4px;}

/* Item =================================== */
$productItemGutter: 30px;

.grid-item .tp-countdown {display: inline-block; width: 100%; position: absolute; bottom: 20px; left: 0; z-index: 1; @include transition;}
.grid-item .tp-countdown .countdown-inner {text-align: center; padding: 0 2px;}
.grid-item .tp-countdown .countdown-inner .countdown-clock>span {margin: 0 1px; max-width: calc(25% - 2px);}
.grid-item .tp-action form.tp-product-grid-cart { margin-top: 15px; float: left;}
.grid-item .tp-product-item-name { margin-bottom: 15px; font-family: $productGridNameFontFamily; font-size: $productGridNameFontSize; font-weight: $productGridNameFontWeight; text-transform: $productGridNameTextTransform; line-height: 1.2; }
.grid-item .tp-product-item-name a:hover { text-decoration: none;}
.grid-item .tp-product-item-name span { display: inline-block; }
.grid-item .tp-product-item-name:hover span {}
.grid-item .spr-badge { margin-bottom: 10px;}
.grid-item .tp-product-item-price { margin-bottom: 10px;}




/* Gird ================================== */
.col-md-9 .tp-product-grid .owl-item .grid-item { width: 100%;}

.tp-product-grid {margin: -($productItemGutter / 2);}
.tp-product-grid:after {content: ''; display: table; clear: both;}
.tp-product-grid .owl-item .grid-item { width: 100%;}
.tp-product-grid .grid-item {width: 100% / $productItemPerRow; float: left; padding: ($productItemGutter / 2);}
.tp-product-grid.slick-slider .grid-item {clear: none !important;}
.tp-product-grid .grid-item .grid-item-inner {position: relative; @include transition; }

.tp-product-grid .grid-image {display: table-cell; vertical-align: middle; text-align: center;}
.tp-product-grid .grid-item .tp-item-des {position: relative; padding-top: 20px; text-align: center;}
.tp-product-grid .spr-badge-caption {display: none;}

@include at-query ($min, 992px) {
  .tp-product-grid .grid-item:nth-child(#{$productItemPerRow}n+1){ clear: left; }
  .col-md-9 .tp-product-grid .grid-item {width: 100% / $productItemPerRow2; clear: none;}
  .col-md-9 .tp-product-grid .grid-item:nth-child(#{$productItemPerRow}n+1){ clear: none; }
  .col-md-9 .tp-product-grid .grid-item:nth-child(#{$productItemPerRow2}n+1){ clear:left; }
}

@media (min-width: 768px) and (max-width: 991px) {
  .tp-product-grid .grid-item {width: 33.333%;}
  .tp-product-grid .grid-item:nth-child(3n+1) ,
  .tp-list-collection .tp-product-grid .grid-item:nth-child(3n+1),
  .col-sm-9 .tp-product-grid .grid-item:nth-child(3n+1) { clear: left;}
}

@include at-query ($max, 767px) {
  .tp-product-grid .grid-item ,
  .tp-list-collection .tp-product-grid .grid-item,
  .col-sm-9 .tp-product-grid .grid-item {width: 50%;}

  .tp-product-grid .grid-item:nth-child(2n+1) ,
  .tp-list-collection .tp-product-grid .grid-item:nth-child(2n+1),
  .col-sm-9 .tp-product-grid .grid-item:nth-child(2n+1) { clear: left;}

  .tp-product-grid .grid-item .spr-badge,
  .tp-product-grid .grid-item .item-color-swatch,
  .tp-product-grid .grid-item .tp-product-item-price,
  .tp-product-grid .grid-item .tp-product-item-name {text-align: center;}

  .tp-product-grid .grid-item .tp-product-item-name {width: 100%; margin-bottom: 10px;}
  .tp-product-grid .grid-item .tp-product-item-price { width: 100%; float: none;  margin-bottom: 5px; clear: both;}
  .tp-product-grid .grid-item .tp-product-item-price s {display: inline;}

  .item-color-swatch ul {display: inline-block;}
  .tp-product-grid .owl-controls {margin: 0 0px; clear: both; }
  .tp-product-grid {margin: 0 -5px;}
  .tp-product-grid .grid-item {padding-left: 5px; padding-right: 5px;}
  .tp-product-grid .tp-des-top {padding: 0;}
  .tp-product-grid .tp-item-des .tp-product-item-price {padding-left: 0; padding-right: 0;}
}

{% if settings.product_item_style == 'style1' %}
/* Product Item Style 1 */
.tp-product-grid .grid-item .tp-action {display: table; table-layout: fixed; width: 100%; background-color: $colorBtnCartBg; z-index: 10;}
.tp-product-grid .grid-item .grid-item-cart {display: table-cell; text-align: center; position: relative;}
.tp-product-grid .grid-item .grid-item-cart:before {content: '\0042';  @include icon-font-peaces(22px); position: absolute; width: 100%; line-height: 40px; text-align: center; color: $colorBtnCartText; pointer-events: none;}

.tp-product-grid .grid-item .add-to-cart-btn {width: 100%; font-size: 0; padding-left: 5px; padding-right: 5px; @include border-radius(0);}

@media (min-width: 992px) {
  .tp-product-grid .grid-item .grid-item-image {overflow: hidden;}
  .tp-product-grid .grid-item .tp-action {position: absolute; bottom: 0; left: 0; @include transform(translateY(100%)); @include opacity(0); @include transition;}
  .tp-product-grid .grid-item .tp-item-des {padding: 20px 10px;}
  .tp-product-grid .grid-item .tp-wishlist-wrap,
  .tp-product-grid .grid-item .tp-compare-wrap {display: table-cell; vertical-align: middle; width: 41px; border-left: 1px solid lighten($colorBtnCartBg,20%);}
  .tp-product-grid .grid-item .tp-wishlist, 
  .tp-product-grid .grid-item .tp-compare {background: $colorBtnCartBg; color: $colorBtnCartText; border: 0 none; @include border-radius(0);}
  .tp-product-grid .grid-item .tp-wishlist:hover, 
  .tp-product-grid .grid-item .tp-compare:hover {background: $colorBtnCartBgHover; color: $colorBtnCartTextHover;}

  .tp-product-grid .grid-item .grid-item-inner:hover {@include box-shadow(0 5px 15px 5px rgba(0,0,0,0.1));}
  .tp-product-grid .grid-item .grid-item-inner:hover .tp-action {@include transform(translateY(0)); @include opacity(1);}
  .tp-product-grid .grid-item .grid-item-inner:hover .tp-countdown {bottom: 50px;}
  
}
@media (max-width: 991px) {
  .tp-product-grid .grid-item .tp-compare-wrap {display: none;}
  .tp-product-grid .grid-item .tp-countdown {bottom: 45px;}
  .tp-compare-wrap, .tp-wishlist-wrap {
    display: block;
    text-align: center;
    border-left: 1px solid #fff;
}
.tp-wishlist {
    font-size: 0;
    display: inline-block;
    text-align: center;
    width: 40px;
    height: 40px;
    line-height: 40px;
    padding: 0;
    background-color: transparent;
  	color:#fff;
    border: none;
    position: relative;
  	padding-top:2px;
    cursor: pointer;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
}
  
  

  
}
@media (max-width: 479px) {
  .grid-item .tp-countdown .countdown-inner .countdown-clock>span {width: calc(25% - 2px); min-width: 0; min-height: 0; padding: 3px;}
}
{% endif %}


/* ##List Product  =========================================================================== */
.tp-product-list .grid-item {clear: both; padding-bottom: 20px; margin-bottom: 20px; border-bottom: 1px solid $colorBorder; @include clearfix;}
.tp-product-list .grid-item:last-child {border-bottom: 0 none;}
.tp-product-list .grid-item .tp-action form.tp-product-grid-cart {float: left; margin-top: 0; margin-right: 15px;}
.tp-product-list .tp-wishlist-wrap {margin-right: 5px;}

.tp-product-list .grid-item .grid-item-image {float: left; margin-right: 10px;  margin-bottom: 0; max-width: 40%;}
.tp-product-list .grid-item .grid-image img:not(:first-child) {display: none;}
.tp-product-list .grid-item .tp-item-des {overflow: hidden; padding: 20px;}
.tp-product-list .grid-item .tp-item-des .tp-action .grid__quickview {display: none !important;}
.tp-product-list .grid-item .tp-product-item-price {margin-bottom: 15px;}
.tp-product-list .item-color-swatch {padding-bottom: 10px;}
.tp-product-list .grid-item .spr-badge {margin-bottom: 15px;}
.tp-product-list .grid-item .grid-item-cart {display: inline-block; margin-right: 5px; vertical-align: top;}
