/* ##HEADING CUSTOM ============================================================= */
$headingPaddingTop : {{ settings.heading_padding_top }}px;
$headingPaddingBottom : {{ settings.heading_padding_bottom }}px;
$heightHeadingLine: {{ settings.height_heading_custom_line }}px;
$widthHeadingLine: {{ settings.width_heading_custom_line }}px;
$headingLineColor: {{ settings.heading_custom_line_color }};

.tp-block-heading,
.tp-home-tab .nav-tabs {
  text-align: $headingCustomPosition; 
  margin-bottom: {{ settings.heading_margin_bottom }}px;
}
.tp-block-heading h3,
.tp-home-tab .nav-tabs>li>a {display: inline-block; margin: 0; line-height: 1; position: relative; border: 0 none;
  font-family: $headingCustomFontFamily; 
  font-size: $headingCustomFontSize; 
  font-weight: $headingCustomFontWeight; 
  font-style: $headingCustomFontStyle; 
  text-transform: $headingCustomCase; 
  padding-top: $headingPaddingTop; 
  padding-bottom: $headingPaddingBottom;  
}
{% unless settings.heading_custom_type == 'none' %}
.tp-block-heading h3:after,
.tp-home-tab .nav-tabs>li.active>a:after { content: ""; position: absolute; border-top-style: solid;
  border-top-color: $headingLineColor; 
  border-top-width: $heightHeadingLine;
  width: $widthHeadingLine;
  max-width: 100%;
  {% case settings.heading_custom_type  %}
    {% when 'top' %}
    top: 0;
    {% else %}
    bottom: 0;
  {% endcase %}
  {% case settings.heading_custom_line_position %}
    {% when 'full' %}
    left: 0; width: 100%;
    {% when 'right' %}
    right: 0;
    {% when 'center' %}
    left: 50%; @include transform(translateX(-50%));
    {% else %}
    left: 0;
  {% endcase %}
}
{% endunless %}
.tp-block-heading h3 a {color: inherit; text-decoration: none;}
.tp-block-heading.text-left {text-align: left;}

.tp-home-tab .nav-tabs {border: 0 none;}
.tp-home-tab .nav-tabs>li {float: none; display: inline-block; padding-left: 15px; padding-right: 15px;}
.tp-home-tab .nav-tabs>li:first-child {padding-left: 0;}
.tp-home-tab .nav-tabs>li:last-child {padding-right: 0;}
.tp-home-tab .nav-tabs>li>a {padding-left: 0; padding-right: 0; color: $colorLight;}
.tp-home-tab .nav-tabs>li.active>a,
.tp-home-tab .nav-tabs>li:hover>a {border: 0 none; background: none; color: $colorHeading;}

@include at-query ($max,991px) {
  .tp-block-heading { text-align: center;}
}

/* ##Breadcrumbs ============================================================ */
.breadcrumb {margin-bottom: 0; padding: 0; background: none; color: $colorBreadcrumbLink; @include border-radius(0); font-size: $baseFontSize - 1px; line-height: 1.2; margin-bottom: 20px; border-top: 1px solid $colorBorder;}
.breadcrumb .breadcrumb-inner {padding: 16px 0;}
.breadcrumb .breadcrumb-inner > a,
.breadcrumb .breadcrumb-inner > span { display: inline; color: $colorBreadcrumbLink; }
.breadcrumb .breadcrumb-inner > a:after {content: '\f105'; font: 14px/1 FontAwesome; display: inline-block; margin: 0 5px 0 7px; @include opacity(0.5);}
.breadcrumb .breadcrumb-inner > strong {color: $colorBreadcrumbText;}
@media (max-width: 991px) {
  .breadcrumb {margin-bottom: 10px;}
}


/* ##Toolbar ======================================================================================= */
.tp-collection-toolbar { margin-bottom: 20px; background-color: #f7f7f7; border: 1px solid $colorBorder; @include border-radius($radius);}
.tp-collection-toolbar:after {content: ''; display: table; clear: both;}

.tp-collection-toolbar .tp-sidebar-aside .sca-qv-button-wrap,
.tp-collection-toolbar .tp-sidebar-aside .sca-qv-button {display: none !important;}
.tp-collection-toolbar .tp-sidebar-aside .tp-button-sidebar-toggle:before { content: "\0052"; @include icon-font-peaces(20px); line-height: 30px; margin-right: 10px; display: inline-block; vertical-align: top; }
.tp-collection-toolbar .tp-sidebar-aside .tp-button-sidebar-toggle {display: block; margin-bottom: 0;}
.tp-collection-toolbar .tp-sidebar-aside .tp-sidebar { position: fixed; top: 0; left: -100%; bottom: 0; height: 100%; height: 100vh; z-index: 99999; overflow-y: auto; overflow-x: hidden;  background-color: #fff;  padding: 20px;  border-right: 1px solid $colorBorder; @include transition(all 500ms);}
.tp-collection-toolbar .tp-sidebar-aside .tp-sidebar .tp-sidebar-block {visibility: visible !important;}
.tp-collection-toolbar .tp-sidebar-aside .tp-sidebar.slide-left {left: 0;}

@include at-query ($min, 992px) {
  .tp-collection-toolbar .tp-sidebar-aside {float: left; }
  .tp-collection-toolbar .tp-sidebar-aside .tp-button-sidebar-toggle {height: 62px; line-height: 62px;  line-height: 60px; margin: -1px; @include border-left-radius($radius);}
  .tp-collection-toolbar .tp-sidebar-aside .tp-button-sidebar-toggle:before {line-height: 60px;}
}

@include at-query ($max, 991px) {
  .tp-collection-toolbar .tp-sidebar-aside .tp-button-sidebar-toggle {margin-bottom: 0; position: fixed; bottom: 0; left: 0; right: 0; z-index: 999; line-height: 30px;}
}

/* Grid/List */
.collection-view { float: left; padding: 10px 15px;}
.collection-view-text {float: left; text-transform: uppercase; line-height: 40px; margin-right: 10px; font-size: 12px; color: $colorLight;}
.change-view { display: block; background: none; float: left; line-height: 1; text-align: center; margin-left: -1px; border: 1px solid $colorBorder; min-width: 40px;}
.change-view .icon-fallback-text {display: block;}
.change-view .icon-fallback-text .fa,
.change-view .icon-fallback-text .fallback-text {display: none;}
.change-view .icon-fallback-text:before {@include icon-font-peaces(16px); line-height: 38px; display: inline-block;}
.change-view[data-view="grid"] {@include border-left-radius($radius);}
.change-view[data-view="list"] {@include border-right-radius($radius);}
.change-view[data-view="grid"] .icon-fallback-text:before {content: '\0045'; }
.change-view[data-view="list"] .icon-fallback-text:before {content: '\0044'; }

.change-view:hover,
.change-view--active { color: $colorPrimary; background-color: #fff;}
.change-view--active {cursor: default;}


/* Sort By */
.tp-collection-sort { float: right; padding: 10px 15px;}
.tp-collection-sort select {height: 40px; background-color: #fff; line-height: 1; color: $colorLight; @include border-radius(0);}
.tp-collection-sort label {margin: 0 10px 0 0; font-size: 12px; line-height: 40px; color: $colorLight; text-transform: uppercase; font-weight: 400; vertical-align: top;}
.tp-collection-sort .dropdown {display: inline-block; min-width: 160px; z-index: 100000;}
.tp-collection-sort .dropdown .dropdown-toggle {display: block; color: $colorLight;}
.tp-collection-sort .dropdown .dropdown-menu {font-size: $baseFontSize - 2px; min-width: 100%; margin-top: 0;}
.tp-collection-sort .dropdown .dropdown-menu li {cursor: pointer;}
.tp-collection-sort .dropdown:hover .dropdown-menu {display: block;}

@include at-query ($max, 991px) {
  .tp-collection-sort select {width: 100px;}
}
@include at-query ($max, 479px) {
  .tp-collection-sort label,
  .collection-view-text {display: none;}
}

/* ##Sidebar Block ================================= */
.section-sidebar+.sca-qv-button-wrap,
.section-sidebar .sca-qv-button-wrap,
.section-sidebar .sca-qv-button {display: none !important; visibility: hidden !important;}
.tp-sidebar-block { margin-bottom: 10px; @include border-radius($radius);}
.tp-sidebar-block:after {content: ''; display: table; clear: both;}
.tp-sidebar-block.tp-sidebar-block-search {margin-bottom: 20px;}
.tp-sidebar-block .tp-sidebar-heading {padding-top: 10px;  padding-bottom: 10px;  border-bottom: 1px solid $colorBorder; margin-bottom: 10px; @include border-top-radius($radius);}
.tp-sidebar-block .tp-sidebar-heading h3 {margin-bottom: 0; font-family: $headingSidebarFontFamily; font-size: $headingSidebarFontSize; font-weight: $headingSidebarFontWeight; text-transform: $headingSidebarTextTransform; position: relative; padding-right: 25px;}
.tp-sidebar-block .tp-sidebar-content {padding: 15px 0; }
.tp-sidebar-block .tp-sidebar-content:after {content:''; display: table; clear: both;}
.tp-sidebar-block .tp-sidebar-content .no-bullets li:not(:last-child) { padding-bottom: 10px; }


/* ## Grid Sidebar */
.grid-sidebar {display: table; width: 100%; table-layout: fixed;}
.grid-sidebar:not(:last-child) {margin-bottom: 20px;}
.grid-sidebar .grid-image {display: table-cell; width: 100px; padding-right: 20px; vertical-align: top;}
.grid-sidebar .tp-item-des {display: table-cell; vertical-align: middle;}
.grid-sidebar .spr-badge-caption {display: none;}
.grid-sidebar .tp-product-item-name {line-height: 1.25; margin-bottom: 10px;}
.grid-sidebar .tp-product-item-price {margin-bottom: 5px;}


/* ##Block Tags  ====================================================================== */
.tp-block-tags ul { margin: 0 -2px;}
.tp-block-tags ul li { display: inline-block; padding: 2px 2px;}
.tp-block-tags ul li a { display: inline-block; line-height: 19px; padding: 8px 15px; @include border-radius ($btnRadius); text-transform: capitalize; background-color: $colorBtnTagBg; color: $colorBtnTagText;}
.tp-block-tags ul li a:hover {background-color: $colorBtnTagBgHover; color: $colorBtnTagTextHover; text-decoration: none;}
.tp-block-tags .more-tags { display: inline-block; text-decoration: underline; padding-top: 10px; font-size: 14px;}


/* ##Block Sidebar Blogs ====================================================================== */
.tp-block-recent-blog .tp-sidebar-content ul {margin-bottom: 0;}
.tp-block-recent-blog .tp-sidebar-content li:not(:last-child) {margin-bottom: 10px;}
.tp-block-recent-blog .tp-sidebar-content li h5 {font-weight: 700; font-size: $baseFontSize; margin-bottom: 0;}
.tp-block-recent-blog .tp-sidebar-content time {font-style: normal; color: $colorLight; font-size: $baseFontSize - 2px;}
.tp-block-recent-blog .tp-sidebar-content .sb-blog-image {overflow: hidden;}
.tp-block-recent-blog .tp-sidebar-content .sb-blog-image {float: left; max-width: 90px; margin-right: 15px;}


/* ##Block Sold By =================================================================== */
.sold-by-inner-top { border-bottom:1px dashed $colorBorder;  text-align:center;}
.sold-by-inner-top p { text-align: left;}
.sold-by-inner-top img { margin-bottom:17px;}

.sold-by-inner-bottom {padding-top:20px; list-style: none; margin: 0;}
.sold-by-inner-bottom li {float:left;width:100%;margin-bottom:20px;}
.sold-by-inner-bottom li:last-child {margin-bottom:0;}
.sold-by-inner-bottom img {float: left;}
.sold-by-inner-bottom .sold-by-caption {overflow: hidden;}
.sold-by-inner-bottom img {margin-right:10px;}
.sold-by-caption h4 {color: $colorHeading; font-size: $baseFontSize - 1px; font-weight: 700;line-height: 1.5; text-transform: capitalize; margin-bottom: 0; }
.sold-by-caption p {font-weight:400;font-size: $baseFontSize - 2px;color: $colorLight;}


/* ##Block Filters =========================================================== */
.tp-filter-block {clear: both;}
.tp-filter-block ul {margin: -5px; list-style: none;}
.tp-filter-block ul li {padding: 5px; }
.tp-filter-block ul li input {display: none;}
.tp-filter-block ul li label {margin-bottom: 0; font-size: $baseFontSize - 1px; font-weight: 400; position: relative; padding-left: 25px;}
.tp-filter-block ul li label:before {content: ""; @extend .icon-font-awesome; width: 16px; height: 16px; line-height: 16px; text-align: center; border: 1px solid; position: absolute; top: 50%; left: 0; margin-top: -8px; font-size: 12px; @include border-radius(2px);}
.tp-filter-block ul li input:checked+label,
.tp-filter-block ul li label:hover {color: $colorPrimary;}
.tp-filter-block ul li input:checked+label:before,
.tp-filter-block ul li label:hover:before {content: "\f00c";}


.tp-sidebar-block .tp-sidebar-heading .fa {display: inline-block; padding: 5px; line-height: 16px; position: absolute; top: 50%; right: -5px; margin-top: -13px; font-size: 18px; cursor: pointer;}
.tp-sidebar-block .tp-sidebar-heading .fa:before {content: '\0046'; @include icon-font-peaces(14px);}
.tp-sidebar-block .tp-sidebar-heading.open .fa:before {content: '\0047';}

.tp-filter-block:last-child .filter-content {padding-bottom: 0;}
.tp-filter-block .tp-filter-content:after,
.tp-filter-block .tp-filter-content ul:after {content: ''; display: table; clear: both;}
.tp-filter-block .tp-filter-content li a:first-letter {text-transform: uppercase;}

.reset-filter {margin-bottom: 25px; cursor: pointer; padding: 12px 12px 12px 20px; font-size: 12px; line-height: 1; text-transform: uppercase; @include border-radius($radius); color: $colorBtnTagText; background-color: $colorBtnTagBg;}
.reset-filter:hover {color: $colorBtnTagTextHover; background-color: $colorBtnTagBgHover;}
.reset-filter:before {content: '\0041'; @include icon-font-peaces(10px); line-height: 12px; margin-right: 8px; vertical-align: top;}

.tp-filter-block.tp-filter-color ul li,
.tp-filter-block.tp-filter-size ul li {display: inline-block;}
.tp-filter-block.tp-filter-color ul li label,
.tp-filter-block.tp-filter-size ul li label {padding-left: 0; display: block; min-width: 35px; height: 35px; line-height: 35px; position: relative; text-align: center; padding: 0 5px; background-size: cover; @include border-radius($radius);}
.tp-filter-block.tp-filter-color ul li label:before,
.tp-filter-block.tp-filter-size ul li label:before {display: none;}
.tp-filter-block.tp-filter-color ul li label:after,
.tp-filter-block.tp-filter-size ul li label:after {content: ""; display: block; width: 100%; height: 100%; border: 1px solid rgba(0,0,0,0.1); pointer-events: none; @include transition; position: absolute; top: 0; left: 0; @include border-radius($radius);}
.tp-filter-block.tp-filter-color ul li input:checked+label:after,
.tp-filter-block.tp-filter-color ul li label:hover:after,
.tp-filter-block.tp-filter-size ul li input:checked+label:after,
.tp-filter-block.tp-filter-size ul li label:hover:after {border-width: 3px; border-color: #000;}

/* Color Variant */
.bg-red {background-color: #d22632 !important;}
.bg-yellow {background-color: #ead84e !important;}
.bg-green {background-color: #57c764 !important;}
.bg-white {background-color: #fff !important;}
